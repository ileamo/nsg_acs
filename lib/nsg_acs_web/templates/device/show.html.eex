<h2>Параметры устройства</h2>

<ul>

  <li>
    <strong>Ключ:</strong>
    <%= @device.key %>
  </li>

  <li>
    <strong>Группа:</strong>
    <%= @device.group.name %>
  </li>

  <h2> Параметры: </h2>

  <%= for {k, v} <- @device.params do %>
  <li>
    <strong><%= k %>:</strong>
    <%= v %>
  </li>
  <% end %>

</ul>

<span><%= link "Edit", to: device_path(@conn, :edit, @device) %></span>
<span><%= link "Back", to: device_path(@conn, :index) %></span>
